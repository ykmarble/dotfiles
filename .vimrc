syntax on
colorscheme peachpuff
set encoding=utf-8
set fileencodings=iso-2022-jp,euc-jp,sjis,utf-8
set fileformats=unix,dos,mac
set nocompatible
set autoindent
set smartindent
set shiftwidth=4
set tabstop=4
set softtabstop=4
set backspace=indent,eol,start
set whichwrap=b,s,h,l,<,>,[,],~
set noswapfile
set nobackup
set wrap
set wrapscan
set wildmenu
set smartcase
set ignorecase
set hlsearch
set expandtab
set incsearch
set showcmd
set showmode
set notitle
set cursorline
set showmatch
set number
set ruler
set laststatus
nnoremap ; :
nnoremap : ;
imap { {}<LEFT>
imap [ []<LEFT>
imap ( ()<LEFT>
set laststatus=2

