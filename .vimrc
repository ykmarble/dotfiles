syntax on
colorscheme hybrid
set nocompatible
set autoindent
set smartindent
set backspace=indent,eol,start
set whichwrap=b,s,h,s,<,>,[,]
set noswapfile
set nobackup
set wrap
set wrapscan
set wildmenu
set smartcase
set ignorecase
set hlsearch
set expandtab
set incsearch
set showcmd
set showmode
set notitle
set cursorline
set showmatch
set number
set ruler
set laststatus
nnoremap ; :
nnoremap : ;
imap { {}<LEFT>
imap [ []<LEFT>
imap ( ()<LEFT>
set laststatus=2
