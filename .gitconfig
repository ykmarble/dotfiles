[user]
    name = yk_marble
    email = milkcocoapple@gmail.com
[core]
    pager = less
    editor = vim
    excludesfile = ~/.gitignore
[alias]
    a = add
    s = status
    st = status
    d = diff
    df = diff
    dif = diff
    c = commit
    cm = commit
    ch = checkout
    mg = marge
    br = branch
    graph = log --graph --date=short --decorate=short --pretty=format:'<%Cblue%h> %Cgreen%cd %Creset%s'
    gr = log --graph --date=short --decorate=short --pretty=format:'<%Cblue%h> %Cgreen%cd %Creset%s'
[color]
    ui = true
    diff = true
[push]
    default = current
